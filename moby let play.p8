function _init()
	x=60
	y=60
end

function _update()
	if (btn(➡️)) x+=1
	if (btn(⬅️)) x-=1
	if (btn(⬆️)) y-=1
	if (btn(⬇️)) y+=1
end

function _draw()
cls()
map(0,0,0,0)
spr(1,x,y)

end
