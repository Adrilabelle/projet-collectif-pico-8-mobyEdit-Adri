version 38
__lua__
function _init()
p={x=60,y=60,s=40}
end

function _update()
	if (btn(➡️)) x+=1
	if (btn(⬅️)) x-=1
	if (btn(⬆️)) y-=1
	if (btn(⬇️)) y+=1
	update_camera()
end

function _draw()
cls()
draw_map()
spr(1,x,y)
spr(p.sprite,p.x*8,p.y*8)
end
-->8
--map

function draw_map()
	map(0,0,0,0,128,64)
	end

function check_flack(flag,x,y)
	local sprite=mget(x,y)
	return fget(sprite,flag)
end

function update_camera()
	camera(x,y)
end

	
